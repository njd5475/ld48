function numcycle()
	local nums={}
	for i=1,4 do
		local c = {
			num=flr(rnd(9)+1),
			mark=false
		}
		nums[c.num]=c
	end
	
	return {
	 
	 draw=function(_, ctx)
	 	drawcycle(nums)
	 end,
	 sheepopts=function(_)
	  local num = nil
		 if flr(rnd(10)) < 3 then
				 num = flr(rnd(9)+1)
				 _.num = num
		 end
		 return num
	 end,
		consume=function(_, o, ctx)
			if o.num > -1 and nums[o.num] then
				local cy = nums[o.num]
				cy.mark = true
				if cy.sheep then
					cy.sheep.children += 1
					sfx(61) -- really good
				else
				 cy.sheep = o
 				sfx(62) -- good
				end
			elseif o.num > -1 and not nums[o.num] then
				sfx(63) -- bad
			else
				sfx(0)
			end
					
			local comp = true
			local amt = 0
			for _, c in pairs(nums) do
				comp = comp and c.mark
				if c.sheep then
					amt += 1 + c.sheep.children
				end
			end

			if comp then
				-- new breeder
				ctx.breeders = ctx.breeders or {}
				ctx.cycle = nil
				--ctx:newcycle()
    ctx._bx = ctx._bx or 4
				local b =breeder(30, amt, ctx._bx, 100)
				sfx(60)
				add(objs, b)
				add(ctx.breeders, b)
				ctx._bx += 16
				
				return nil
			end
			
			return _
		end
	}
end

