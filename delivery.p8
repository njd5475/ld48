-- calculations

function totime(cnt)
	local secs,mins = 0,flr(cnt/60)
	secs = flr(cnt % 60)
	return mins, secs
end

function numbrds(brds, t)
 brds = brds or {}
	local amt=0
	for b in all(brds) do
		if b._type == t then
			amt+=1
		end
	end
	return amt
end
