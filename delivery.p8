function sheep(x,y,opts)
 local _num, _cycle, _clr = nil, nil, 7
 if type(opts) == 'table' then
  if opts._type == 'cycle' then
	  _cycle = opts.cycle
	 elseif opts._type == 'clrcycle' then
	  _clr = opts.clr
	  _cycle = opts.cycle
	 elseif opts._type == 'clr' then
	 	_clr = opts.clr
	 end
 else
  _num = opts
 end
 local osc,st=0,true
 
 return {
  _type='sheep',
  reward=1,
  clr=_clr, --rnd({7,0,15,4,5}),
  num=_num or -1,
  cycle=_cycle,
  children=0,
  pos=function()
  	return x,y
  end,
  draw=function(_)
  	_:_draw(x,y)
  end,
  _draw=function(_, x, y)
     pal(7,_.clr)
     local frx,fry,sep=x+3,y+7,3
     local fr,bk = pget(frx,fry), pget(frx+sep,fry)
     spr(1, x, y)
					if st then
					 pset(frx,fry-1, 7)
	     pset(frx,fry,fr)
     else
      pset(frx+sep,fry-1, 7)
 	    pset(frx+sep,fry,bk)
     end
     if _.children > 0 then
	     spr(2, x-2, y+2)
     end
     pal(7,7)
     if _.cycle then
     	_.cycle:draw(x,y)
     end
  	 	if _.num > 0 then
		   	print(_.num, x, y-8, 14)
  		 end
  		 --print(spd, x, y, 15)
  end,
  update=function(me, ctx)
    x-=1
    if x < -8 then
      del(objs, me)
    end
    osc += 1
    if osc % spd == 0 then
    	st = not st
    end
  end,
  hit=function(me, ox, oy)
  	return col(x,y,ox,oy)
  end
 }
end

