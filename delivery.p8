	 amt=flr(amt),
	 _type=_t,
		draw=function(_, ctx)
			--print('+' .. amt .. '/' .. rate, x, y) 
			spr(_t, x, y, 2, 2)
			--print(_.rate, x, y+8, 12)
			--print(_.amt, x, y, 12)
			--print(t, x+8, y, 12)
		end,
		update=function(_, ctx)
		 t+=1
		 
		 if t >= _.rate then
				ctx.sheep += amt
				add(objs, floater(x,y,amt))
