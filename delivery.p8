function pillow(x,y)
	return {
	 _type='pillow',
		draw=function(_, ctx)
			spr(18, x,y)
			if ctx.pause then
				print('press üÖæÔ∏è to unpause', 16, 64)
			end			
		end,
		update=function(me, ctx)
			if btn(2) then
				y -= 1
			end
			if btn(3) then
				y += 1
			end
					
			y = min(16*5+2,max(30,y))
			for o in all(objs) do
				-- check collisions
				if o._type =='sheep' and o:hit(x,y) then
					-- collided
					del(objs, o)

     ctx.sheep += 1 + (me.children or 0)

     if o.cycle then
      -- switch cycle
     	ctx.cycle = o.cycle.cycle()
     elseif ctx.cycle then
     	ctx.cycle = ctx.cycle:consume(o, ctx)
		   end
				end	
			end
		end,
		_pupdate=function(_, ctx)
			if btnp(4) then
				ctx.pause=false
			end
		end,
	}
end

